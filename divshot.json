{
  "name": "wordset",
  "root": "./dist",
  "prerender": true,
  "routes": {
    "/**": "index.html"
  },
  "proxy": {
    "data": {
      "origin": "https://data.wordset.org",
      "cookies": false,
      "headers": {
        "content-type": "text/javascript"
      },
      "timeout": 30
    }
  }
}
