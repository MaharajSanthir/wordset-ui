<h2>Seqs</h2>

{{#each changes.seqs as |seq|}}
  {{edit-seq seq=seq}}
  {{edit-labels labels=lang.labels selectedLabels=seq.labels}}
{{/each}}

<h2>Meanings</h2>

{{#each changes.meanings as |meaning|}}
  {{edit-meaning meaning=meaning}}
  {{edit-labels labels=lang.labels selectedLabels=meaning.labels }}
{{/each}}

<label>
  Reason and sources
  {{textarea value=reason}}
</label>
