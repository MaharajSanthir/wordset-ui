<div class="meaning-labels">
  {{#if sameLabels}}
    <div class="label-grouping">
      Existing Labels:
      {{#each sameLabels as |label|}}
        <div {{bind-attr class=":label label.isDialect:dialect:nondialect"}}>{{label.name}}</div>
      {{/each}}
    </div>
  {{/if}}
  {{#if addedLabels}}
    <div class="label-grouping">
      <ins>Added Labels:</ins>
      {{#each addedLabels as |label|}}
        <div {{bind-attr class=":label label.isDialect:dialect:nondialect"}}>{{label.name}}</div>
      {{/each}}
    </div>
  {{/if}}
  {{#if removedLabels}}
    <div class="label-grouping">
      <del>Removed Labels:</del>
      {{#each removedLabels as |label|}}
        <div {{bind-attr class=":label label.isDialect:dialect:nondialect"}}>{{label.name}}</div>
      {{/each}}
    </div>
  {{/if}}

</div>
