<li>
  <div class="info">
    <div class="meaning">
      {{#suggestable-area model=meaning field=meaning.def}}
        <div class="def">{{meaning.def}}</div>
      {{/suggestable-area}}

      {{#if meaning.quotes}}
        <div class="quoteWrap">
          {{#each quote in meaning.quotes}}
            {{quote-display quote=quote}}
          {{/each}}
        </div>
      {{/if}}
    </div>
  </div>
</li>
