<div class="wrapper">
  <h2>New Word Proposal</h2>

  <div class="word">
    {{model.wordName}}
  </div>
  <div class="proposal novel">
    {{#each meaning in meanings}}
      <div class="pos">
        {{meaning.pos}}
      </div>
      <div class="info">
        <div class="definition">
          <div class="def">
            {{meaning.def}}
          </div>
        </div>
        <div class="quoteWrap">
          <div class="quote highlight">
            <span class="quotemark"> “ </span>
            {{meaning.example}}
            <span class="quotemark"> ” </span>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>
