<div class="word">
  {{#link-to "seq.wordset.index" model.lang.id model.wordName}}
    {{model.wordName}}
  {{/link-to}}
</div>
<div class="pos">
  {{model.original.pos}}
</div>


{{label-diff newLabels=model.labels originalLabels=model.originalLabels}}

{{#if isEditing}}
  {{partial "proposal/edit-meaning-change-form"}}
{{else}}
  <div class="info">
    <div class="definition">
      <div class="def">
        {{diff-text model.original.def model.def}}
      </div>
    </div>
    <div class="quoteWrap">
      <div class="quote highlight">
        <span class="quotemark"> “ </span>
          {{diff-text model.original.example model.example}}
        <span class="quotemark"> ” </span>
      </div>
    </div>
  </div>
{{/if}}
